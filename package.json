{
  "name": "prohub",
  "version": "1.1.0",
  "main": "app/index.js",
  "scripts": {
    "start": "node app/index.js",
    "bundle": "npm run bundle-css & npm run bundle-js",
    "watch": "npm run watch-css & npm run watch-js & npm run start",
    "bundle-css": "stylus ./app/styles/index.styl -o ./app/assets/css",
    "bundle-js": "browserify ./app/client.js > ./app/assets/js/index.js",
    "watch-css": "stylus -w ./app/styles/index.styl -o ./app/assets/css",
    "watch-js": "watchify ./app/client.js -o ./app/assets/js/index.js"
  },
  "dependencies": {
    "JSONStream": "^0.10.0",
    "browserify": "^8.0.3",
    "c3": "^0.4.7",
    "class-list": "^0.1.1",
    "cookie": "^0.1.2",
    "dom-events": "^0.1.1",
    "dom-remove": "^1.1.0",
    "domready": "^1.0.7",
    "ecstatic": "^0.5.5",
    "engine.io-stream": "^0.4.3",
    "github-cache": "^0.3.0",
    "github-oauth": "^0.2.0",
    "hyperquest": "^1.0.1",
    "inherits": "^2.0.1",
    "insert-css": "^0.2.0",
    "jade": "^1.7.0",
    "jadeify": "^4.0.0",
    "level": "^0.18.0",
    "level-sublevel": "^6.3.17",
    "level-updater": "^0.1.0",
    "levelup": "^0.19.0",
    "memoizeasync": "^0.8.0",
    "moment": "^2.8.4",
    "node-time-uuid": "^0.1.4",
    "paramify": "^0.1.2",
    "rc": "^0.5.4",
    "reconnect-engine": "0.0.1",
    "split": "^0.3.2",
    "stream-to-array": "^2.0.2",
    "stylus": "^0.49.3",
    "through": "^2.3.6",
    "through-parse": "^0.1.0",
    "through2": "^0.6.3",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "jadeify": "^4.0.0",
    "watchify": "^2.2.1"
  }
}
